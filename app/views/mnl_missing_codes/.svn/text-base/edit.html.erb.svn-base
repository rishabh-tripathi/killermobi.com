<h1>Editing mnl_missing_code</h1>

<%= error_messages_for :mnl_missing_code %>

<% form_for(@mnl_missing_code) do |f| %>
  <p>
    <b>Code</b><br />
    <%= f.text_field :code %>
  </p>

  <p>
    <b>Detail</b><br />
    <%= f.text_field :detail %>
  </p>

  <p>
    <%= f.submit "Update" %>
