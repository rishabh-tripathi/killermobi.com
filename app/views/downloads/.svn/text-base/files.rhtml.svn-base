
<div class="download_section">
<div id="download_section_top">Free Mobile Softwares @ KillerMobi</div>
<div class="pwrd_by"><%= link_to 'Back to Free Mobile Software Home', root_path %></div>
<div class="show_articles">

<h1>File Upload</h1>

<% form_tag ({:action => 'uploadFile'}, :multipart => true) do%>

<%= hidden_field_tag 'download_id',@id %>

<p><label for="upload_file">Select JAR file</label> :
<%= file_field 'upload', 'datafile' %></p>

<p><label for="upload_file">Select JAD file</label> :
<%= file_field 'upload1', 'datafile' %></p>

<H2>Or</h2>

<p><p>Enter Url to download</p> :
<%= text_field 'uploadurl', 'datafile' %></p>
<br>

<h3>Select Screenshots</h3>

<p><label for="upload_file">Select Screen Shot1 file</label> :
<%= file_field 'ss1', 'datafile' %></p>

<p><label for="upload_file">Select Screen Shot2 file</label> :
<%= file_field 'ss2', 'datafile' %></p>

<p><label for="upload_file">Select Screen Shot3 file</label> :
<%= file_field 'ss3', 'datafile' %></p>

<%= submit_tag "Upload" %>
<% end %>
</div></div>
