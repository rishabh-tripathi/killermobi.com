<!-- Menu designing ---------->
<div id="wrap">
  <div style="position:relative">
    <div id="menu_download" class="hp-menu" style="left:110px;width:500px;display:none;" onmouseover="menuMouseOver('download_menu');document.getElementById('menu_download').show();" onmouseout="menuMouseOut('download_menu');document.getElementById('menu_download').hide();">
      <div style="font-size:16px;line-height:16px;margin:-10px 0px -2px 100px;">
        <img src="/images/v2/up.png" style="width:16px;height:16px;" />
      </div>
      <div class="round-st-border">
        <div class="round-border-content">
          <h2 style="margin-left:10px;"><%= link_to 'All Downloads', download_url %></h2>
          <hr>
            <table style="vertical-align:top;width:100%;">
              <tr>
                <td style="width:45%;padding:10px;border-right:1px solid #e0e0e0;">
                  <h2>Categories</h2>
                  <ul>
                    <% Download::CAT_NAMES.sort.each do|key, value| %>
                    <li><a href="<%= category_url(:category => value) %>"><h3><%= value -%></h3></a></li>
                    <% end %>
                  </ul>
                </td>
                <td style="padding:10px;">
                  <h2>Platforms</h2>
                  <ul>
                    <% Download::PLATFORM_NAMES.sort.each do|key, value| %>
                    <li><a href="<%= platform_url(:type => value.gsub(' ','_')) %>"><h3><%= value %><h3></a></li>
                    <% end %>
                  </ul>
                </td>
              </tr>
            </table>
        </div>
      </div>
    </div>


    <% if @home %>
    <div id="menu_forum" class="hp-menu" style="left:220px;width:150px;display:none;" onmouseover="menuMouseOver('forum_menu');document.getElementById('menu_forum').show();" onmouseout="menuMouseOut('forum_menu');document.getElementById('menu_forum').hide();">
      <div style="font-size:16px;line-height:16px;margin:-10px 0px -2px 75px;">
        <img src="/images/v2/up.png" style="width:16px;height:16px;"/>
      </div>
      <div class="round-st-border">
        <div class="round-border-content">
          <h2><%= link_to 'All Forums', forums_url %></h2>
          <hr>
            <ul>
              <% @forum.each do |forum| -%>
              <li><h3><%= link_to forum.name, topics_path(:forum_id => forum.id) -%></h3></li>
              <% end %>
            </ul>
        </div>
      </div>
    </div>

    <div id="menu_article" class="hp-menu" style="left:330px;width:150px;display:none;" onmouseover="menuMouseOver('article_menu');document.getElementById('menu_article').show();" onmouseout="menuMouseOut('article_menu');document.getElementById('menu_article').hide();">
      <div style="font-size:16px;line-height:16px;margin:-10px 0px -2px 60px;">
        <img src="/images/v2/up.png" style="width:16px;height:16px;"/>
      </div>
      <div class="round-st-border">
        <div class="round-border-content">
          <h2><%= link_to 'All Articles', articles_url %></h2>
          <hr/>
          <ul>
            <% @article_category.each do |category| -%>
            <li><h3><%= link_to category.name, article_cat_url(category.name.tr(" ","_")) %></h3></li>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
    <% end %>

    <% if user_signed_in?
         width_str = "100px"
         margin_str = "60px"
       else
         width_str = "300px"
         margin_str = "260px"
       end
     %>

    <div id="menu_user" class="hp-menu" style="right:10px;width:<%= width_str -%>;display:none;" onmouseover="menuMouseOver('user_menu');document.getElementById('menu_user').show();" onmouseout="menuMouseOut('user_menu');document.getElementById('menu_user').hide();">
      <div style="font-size:16px;line-height:16px;margin:-8px 0px -2px <%= margin_str -%>;">
        <img src="/images/v2/up.png" style="width:16px;height:16px;" />
      </div>
      <div class="round-st-border">
        <div class="round-border-content">
          <% if user_signed_in? %>
          <%= link_to 'Edit Profile', edit_user_registration_url(current_user) %> <br/>
          <%= link_to 'Logout', logout_url %>
          <% else %>
            <%= render :partial => 'partials/login' %>
          <% end %>
        </div>
      </div>
    </div>

  </div>
</div>
<!-- Menu designing ends here ---------->
</div><!-- End Of Top Background -->


<% end %>
